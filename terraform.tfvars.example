aws_region     = "us-east-1"
project_prefix = "your-project"
env            = "dev"

api_instance_type = "t3.micro"

db_instance_class    = "db.t3.micro"
db_engine            = "mysql"
db_engine_version    = "8.0"
db_allocated_storage = 20

admin_email = "your-email@example.com"

ec2_instance_profile_name = "your-ec2-profile"
backup_bucket_name        = "your-backup-bucket"
alerts_sns_name           = "your-alert-topic"

ssh_cidr    = "YOUR_PUBLIC_IP/32"
CWA_DEB_URL = "https://s3.amazonaws.com/amazoncloudwatch-agent/ubuntu/amd64/latest/amazon-cloudwatch-agent.deb"
